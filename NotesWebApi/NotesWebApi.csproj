    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\NotesApplication\NotesApplication.csproj" />
    <ProjectReference Include="..\NotesPersistence\NotesPersistence.csproj" />
  </ItemGroup>

</Project>
